<!--Page that will be rendered for error messages-->

<% layout('layout/boilerplate')  %> 
<div class="row">
    <div class="col-6 offset-3">
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"><%= err.message %> </h4>
            <% if(process.env.NODE_ENV == 'production'){%>
                <p><%= err.stack %> </p>
           <% } %>
        </div>

        <img class="img-fluid" alt="" src='https://res.cloudinary.com/dfonrynhp/image/upload/v1612314398/CampSights/Icons/gummy-camping_vmtvlf.png'>

    </div>
</div>
